<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".utama">

    <TextView
        android:id="@+id/ambilUsername"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="148dp"
        android:text="Username"
        android:textAlignment="center"
        android:textSize="20dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/ambilNama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama"
        android:textAlignment="center"
        android:textSize="20dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@id/ambilUsername"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/ambilLahir"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lahir"
        android:textAlignment="center"
        android:textSize="20dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@id/ambilNama"
        tools:layout_editor_absoluteX="0dp" />
    <TextView
        android:id="@+id/ambilDarah"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Golongan Darah"
        android:textAlignment="center"
        android:textSize="20dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@id/ambilLahir"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/ambilAlamat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Alamat"
        android:textAlignment="center"
        android:textSize="20dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@id/ambilDarah"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/ambilNo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="No"
        android:textAlignment="center"
        android:textSize="20dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@id/ambilAlamat"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/ambilPoin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Poin"
        android:textAlignment="center"
        android:textSize="20dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@id/ambilNo"
        tools:layout_editor_absoluteX="0dp" />
    <TextView
        android:id="@+id/ambilNik"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="NIK"
        android:textAlignment="center"
        android:textSize="20dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@id/ambilPoin"
        tools:layout_editor_absoluteX="0dp" />




</androidx.constraintlayout.widget.ConstraintLayout>